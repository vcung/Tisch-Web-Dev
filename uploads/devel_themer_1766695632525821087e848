-creates-indelible-experiences" data-thmr="thmr_28 thmr_31"><img src="http://localhost:8888/forSelbert/Tisch-Web-Dev/drupal/sites/default/files/styles/highlight_front/public/Liat%20Litwin%2C%20A14%20-%20photo%20credit%20-%20Kelvin%20Ma.jpeg?c=eda61398436feb94ff27226792d09b3d" width="235" height="165" alt="Liat Litwin records in Digital Design Studio" data-thmr="thmr_29 thmr_30" /></a>";}s:4:"type";s:4:"func";}s:7:"thmr_32";a:2:{s:9:"variables";a:6:{s:4:"view";r:319;s:5:"field";O:25:"views_handler_field_field":25:{s:5:"items";a:0:{}s:10:"field_info";a:16:{s:12:"translatable";s:1:"0";s:12:"entity_types";a:0:{}s:8:"settings";a:1:{s:10:"max_length";s:2:"50";}s:7:"storage";a:5:{s:4:"type";s:17:"field_sql_storage";s:8:"settings";a:0:{}s:6:"module";s:17:"field_sql_storage";s:6:"active";s:1:"1";s:7:"details";a:1:{s:3:"sql";a:2:{s:18:"FIELD_LOAD_CURRENT";a:1:{s:30:"field_data_field_front_tagline";a:2:{s:5:"value";s:25:"field_front_tagline_value";s:6:"format";s:26:"field_front_tagline_format";}}s:19:"FIELD_LOAD_REVISION";a:1:{s:34:"field_revision_field_front_tagline";a:2:{s:5:"value";s:25:"field_front_tagline_value";s:6:"format";s:26:"field_front_tagline_format";}}}}}s:12:"foreign keys";a:1:{s:6:"format";a:2:{s:5:"table";s:13:"filter_format";s:7:"columns";a:1:{s:6:"format";s:6:"format";}}}s:7:"indexes";a:1:{s:6:"format";a:1:{i:0;s:6:"format";}}s:2:"id";s:2:"54";s:10:"field_name";s:19:"field_front_tagline";s:4:"type";s:4:"text";s:6:"module";s:4:"text";s:6:"active";s:1:"1";s:6:"locked";s:1:"0";s:11:"cardinality";s:1:"1";s:7:"deleted";s:1:"0";s:7:"columns";a:2:{s:5:"value";a:3:{s:4:"type";s:7:"varchar";s:6:"length";s:2:"50";s:8:"not null";b:0;}s:6:"format";a:3:{s:4:"type";s:7:"varchar";s:6:"length";i:255;s:8:"not null";b:0;}}s:7:"bundles";a:1:{s:4:"node";a:1:{i:0;s:9:"highlight";}}}s:8:"multiple";b:0;s:12:"limit_values";b:0;s:10:"base_table";s:4:"node";s:8:"instance";N;s:11:"field_alias";s:3:"nid";s:7:"aliases";a:1:{s:11:"entity_type";s:47:"field_data_field_front_tagline_node_en