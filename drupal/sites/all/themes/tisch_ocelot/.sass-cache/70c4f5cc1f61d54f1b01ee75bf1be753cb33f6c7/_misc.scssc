    li.hover, li:hover {
      position: relative; 
    }     
          
    /*second level dropdown menu*/
    ul{
      visibility:hidden;
      z-index:9999;
      position: absolute;   
      width:200px;
      li {
        float: none;
        margin-left: -1px;
        background: black;
        text-align: left;
