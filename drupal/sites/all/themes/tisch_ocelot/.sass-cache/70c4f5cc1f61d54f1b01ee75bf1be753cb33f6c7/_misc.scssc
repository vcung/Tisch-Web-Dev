    div {
      border-bottom: 2px solid $dark-gray;
      margin-left:0;
      padding-left:0;      
    }  
    div:last-child{
      border-bottom:none;
    }
    h3{
      margin-left:0;
      padding-left:0;
    }
    .socialIcons p {
      margin-bottom: 2em;  
    }   
  }
  
  div{
    @include leader();
  }
  .socialIcons{
    margin-left: 0px;
    padding-left: 0px;
    @extend .backgroundFontIcon;
    @extend .socialMediaBoxes;
    p{
      color: white;
      background-color: $dark-gray;
    }
      
    a:link{
      color: white;
      text-decoration: none;
    }
    a:hover {
      color: $heading-orange;
    }
    a:visited{
      color: white;
      text-decoration: none;
    }
  }

       
  #footerLogo{
    background-image: url("../images/tuftsLogo.jpg");
    height: 150px;
    background-repeat: no-repeat; }
    padding-bottom: 2%;
  }

  //fix width for logo area to not overlap the background image
  @media all and (min-width:600px) and (max-width:710px){
    #footerLogo{
      min-width: 20%;
    }
  }
  @media all and (min-width: 600px)  {
    #footerContent{ 
      margin-left:auto;
      margin-right:auto;
     @include zen-clear();
     $zen-column-count: 7;
    //$zen-gutter-width: 10px;
    @include zen-grid-container;
    width: 100%;
    div {
      margin-bottom: 5%;
      p {
        padding-left: 5%;
      }
      height: 150px;
      border-right: 1px solid $dark-gray;
    }
    
    div:last-child{
      border-right:none;
    }
  
    #footerLogo{/* Span 1 columns, starting in 1st column from left. */
      @include zen-grid-item(1, 1);
    }
  
    #otherFooterLibs {/* Span 1 columns, starting in 2nd column from left. */
    @include zen-grid-item(2, 2);
    }

    #otherFooterTufts {/* Span 1 column, starting in 4th column from left. */
    @include zen-grid-item(1, 4);
    }
  
    #footerContact {/* Span 1 columns, starting in 2nd column from left. */
      @include zen-grid-item(1, 5);
      p.address{
        margin:0px;
      }
      p {
        margin:0.5em 0 0.5em 0;
      }
    }

    #footerSocial {/* Span 2 columns, starting in 6th column from left. */
      @include zen-grid-item(2, 6);
      //give 1st icon a bigger margin once footer is horizontal
      .socialIcons p:first-of-type{
        margin-left: 5%;
      }
    }
  }//endfooter large screen
}//end footer

/**
 * Wireframes.
 */
.with-wireframes {
  #header,
  #main,
  #content,
  #navigation,
  .region-sidebar-first,
  .region-sidebar-second,
  #footer,
  .region-bottom {
    outline: 1px solid #ccc;

    @if $legacy-support-for-ie6 or $legacy-support-for-ie7 {
      .lt-ie8 & {
        /* IE6/7 do not support the outline property. */
        border: 1px solid #ccc;
      }
    }
  }
