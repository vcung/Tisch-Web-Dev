/**
 * Miscellanious styling classes that are extended by other selectors in the document
 */
.whiteText {
  color: white; 
 }

.shadow{
  @include single-text-shadow;
 }

.whiteCaptions{
  color: white;
  @include adjust-font-size-to(14px);
  padding-left: 2%;
  font-weight: 550;
  line-height: 1em;
}

.whiteLink {
  a:link{
    color: white;
    text-decoration:none;
  }
  a:visited{
    color:white;
  }
}

.blackLink{
  a:link{
    color: black;
    text-decoration:none;
  }
  a:visited{
    color:black;
  }
}
 
/*Headings that serve as titles to blocks in sidebars like within Happening Now section*/
.smallerHeading{
  @include adjust-font-size-to (14px);
  text-transform: uppercase;
  margin-bottom: 0;
  padding-bottom: 0;
  padding-left: $padding-amount;
  word-break: keep-all;
}
.smallerBlueHeading{
  @extend .smallerHeading;
  color: $dark_blue_bg;
}
.smallerBlackHeading{
 @extend .smallerHeading;
  color: black;
}

/*heading that will get a block background that is either blue or green and may have some
**additional background images*/
.mainHeading{
  h2{
    @include adjust-font-size-to(18px);
    font-weight: normal;
    text-transform: uppercase;
    padding-top:0;
    padding-bottom:0;
    line-height:1.6em;
    padding-left: 3px;
    color: white;
  }
}

/*Provide heading for "Chat with a librarian" and "Top Resources"*/
.blueBlockHeading{
  @extend .mainHeading;

  h2{
    background-color: $blue_bg;
    overflow:hidden;
    white-space: nowrap;
    @media all and (max-width:995px){
      @include adjust-font-size-to(16px);
      line-height:1.8em;
    }
  }
}

/*Bring in web font icons*/
.backgroundFontIcon {
