    //@extend .smallerBlackHeading;
    
   }
}
.field-content{
  //padding: 2%;
  //margin: 2%;
}


}
/***
*   SPECIFIC NODES & CONTENT TYPES
***/

/*KB added*/
.node-basic-content {

  h2 {
    color: darken($tufts-classic, 20%);
    text-transform: uppercase;
    padding-left: $padding-amount;
    padding-right: 0;

  }
  //display: inline;
  background-color: white;
  margin-bottom: $vertical-content-amount;
  @include box-shadow(gray 3px 3px 7px);
.field-name-body{
   //background-color: white;
   padding-left: $padding-amount;
   float: left;
   //width: 100%;
}
  

 
.field-name-field-2nd-column-body{
  @media all and (min-width: 480px) {

  float: right;
  padding-right: $padding-amount;
  z-index: -10;
  h5 {
     //color: green;
     padding-top: $base-line-height * 3;
   }
 }
}

